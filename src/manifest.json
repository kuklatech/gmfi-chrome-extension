{
  "name": "GMFIndex",
  "description": "Inform about mission and sustainable certification if the company has any",
  "version": "1.0.9",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "webNavigation", "storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo-inactive/logo-inactive-with-badge-16.png",
      "32": "/images/logo-inactive/logo-inactive-with-badge-32.png",
      "48": "/images/logo-inactive/logo-inactive-with-badge-48.png",
      "128": "/images/logo-inactive/logo-inactive-with-badge-128.png"
    }
  },
  "icons": {
    "16": "/images/logo-inactive/logo-inactive-with-badge-16.png",
    "32": "/images/logo-inactive/logo-inactive-with-badge-32.png",
    "48": "/images/logo-inactive/logo-inactive-with-badge-48.png",
    "128": "/images/logo-inactive/logo-inactive-with-badge-128.png"
  }
}
